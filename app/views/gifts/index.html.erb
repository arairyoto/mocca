<a href="/admin">Admin Home</a>

<h1>Listing Gifts</h1>

<table class="table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Url</th>
      <th>Img</th>
      <th>Img content type</th>
      <th>Company Name</th>
      <th>Price</th>
      <th>Dislikeの回数</th>
      <th>Likeの回数</th>
      <th>Likeの割合</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gifts.each do |gift| %>
      <tr>
        <td><%= gift.id %></td>
        <td><%= gift.name %></td>
        <td><%= gift.url[0,10]+"..." %></td>
        <td><%= image_tag(img_gift_path(gift), :height => "100px") %></td>
        <td><%= gift.img_content_type %></td>
        <td><%= gift.company_name %></td>
        <td><%= gift.price %></td>
        <td><%= @giftEval1[gift] %></td>
        <td><%= @giftEval2[gift] %></td>
        <td><%= 1.0*@giftEval2[gift]/(@giftEval1[gift]+@giftEval2[gift]) %></td>
        <td><%= link_to 'Show', gift %></td>
        <td><%= link_to 'Edit', edit_gift_path(gift) %></td>
        <td><%= link_to 'Destroy', gift, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

